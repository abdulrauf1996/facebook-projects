<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.qwe.acer.facebooknew.Action">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#5db6f1">

    <ImageButton
               android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/homee"
        android:id="@+id/imageButton7"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/frnds"

        android:id="@+id/imageButton8"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton7"
        android:layout_toEndOf="@+id/imageButton7" />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/babbu"
        android:id="@+id/imageButton9"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/imageButton10"
        android:layout_toStartOf="@+id/imageButton10"
        android:contentDescription="1" />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/not"
        android:id="@+id/imageButton10"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/imageButton11"
        android:layout_toStartOf="@+id/imageButton11" />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/zoom"

        android:id="@+id/imageButton11"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/imageButton12"
        android:layout_toStartOf="@+id/imageButton12" />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/multiple_choice1600"
        android:id="@+id/imageButton12"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>


</RelativeLayout>

